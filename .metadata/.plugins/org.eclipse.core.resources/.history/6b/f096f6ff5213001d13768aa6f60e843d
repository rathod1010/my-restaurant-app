server:
  port: 9090
eureka:
 client:
   service-url:
    defaultZone: http://localhost:8761/eureka/
spring:
 application:
  name: GATEWAY-SERVICE
 cloud:
    gateway:
      routes:
      - id: restaurantModule
        uri: http://localhost:8090/
        predicates:
        - Path=/restaurant/**
      - id: reservationModule
        uri: http://localhost:8091/
        predicates:
        - Path=/reservation/**
        
      - id: userModule
        uri: http://localhost:8092/
        predicates:
        - Path=/user/**